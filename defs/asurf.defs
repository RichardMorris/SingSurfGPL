<group name="Cubics">

<group name="Smooth cubics">
<definition name="Diagonal Surface of Clebsch" type="asurf">
//# This surface contains 27 straight lines

16 x^3 + 16 y^3 - 31 z^3 + 24 x^2 z - 48 x^2 y - 48 x y^2
+24 y^2 z - 54 sqrt(3) z^2 - 72 z;
<variable name="x" min="-4.14" max="4.03">
<variable name="y" min="-4.13" max="4.04">
<variable name="z" min="-4.12" max="4.05">
</definition>


<definition name="Fermat Cubic" type="asurf">
// http://en.wikipedia.org/wiki/Fermat_cubic
x^3+y^3+z^3+w^3;
<parameter name="w" value="-1"/>
<variable name="x" min="-2.14" max="2.03">
<variable name="y" min="-2.13" max="2.04">
<variable name="z" min="-2.12" max="2.05">
</definition>


</group>

<group name="Class II: one A1 singularity">

<definition name="Hunt A1" type="asurf">
//# Cubic with an A1 singularity
x y + x z + y z + x y z - 0;
<variable name="x" min="-1.14" max="1.03">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
</definition>

<definition name="CayleyII" type="asurf">
//<h3>Class II: 1 A<sub>1</sub> singularity</h3>
//<p>In Cayley's notation: II = 12 - C2; 	
//W(a,b,c,f,g,h|X, Y, Z)^2+2 k X Y Z;
W (a X^2 + b Y^2 + c Z^2 + f X Y+ g X Z+h Y Z)+2 k X Y Z;
X = x + y + z + w;
Y = -x - y + z + w;
Z = x -y -z + w;
W = -x + y - z + w;
w = 1;
<parameter name="a" value="0.9" />
<parameter name="b" value="-0.3" />
<parameter name="c" value="0.8" />
<parameter name="f" value="-0.2" />
<parameter name="g" value="1.0" />
<parameter name="h" value="0.8" />
<parameter name="k" value="0.4" />
<variable name="x" min="-3.14" max="3.03">
<variable name="y" min="-3.13" max="3.04">
<variable name="z" min="-3.12" max="3.05">
</definition>

</group>

<group name="Multiple A1 singularities">

<definition name="Cayley's Cubic" type="asurf">
//# A cubic surface with 4 nodes
//# all other cubics with 4 nodes are projectivly equivilent to this one
4 (x^2+y^2+z^2) + 16 x y z - 1;
<variable name="x" min="-1.14" max="1.03">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
</definition>

<definition name="S3" type="asurf">
// http://www.dm.unito.it/modelli/modelli/s3.html
// Conical surface with four double points obtained from the finite real Cayley surface by a homographic transformation or projectivity. 
9 x^3 + 3 x^2 z - 4 z^3 - 27 y^2 x + 3 y^2 z - 4 ( 9 x^2 + 9 y^2 - 4 z^2)(2 z+1)/7;
<variable name="x" min="-6.14" max="6.03">
<variable name="y" min="-6.13" max="6.04">
<variable name="z" min="-6.12" max="6.05">
</definition>

<definition name="S4" type="asurf">
// http://www.dm.unito.it/modelli/modelli/s4.html
// Conical surface with four double points obtained from the finite real Cayley surface by a homographic transformation or projectivity. 
9 x^3 + 18 x^2 + 15 x^2 z - 27 x y^2 + 18 y^2 + 15 y^2 z- 4 z^3 - 8 z^2;
<variable name="x" min="-5.01" max="5.03">
<variable name="y" min="-5.13" max="5.14">
<variable name="z" min="-5.07" max="5.02">
</definition>

<definition name="S5" type="asurf">
// http://www.dm.unito.it/modelli/modelli/s5.html
// Conical surface with four double points obtained from the finite real Cayley surface by a homographic transformation or projectivity. 
9 x^3 - 3 x^2 z + 4 z^3 - 27 y^2 x - 3 y^2 z - 4 (9 x^2+9 y^2-4 z^2)(z+4);
<variable name="x" min="-10.14" max="10.03">
<variable name="y" min="-10.13" max="10.04">
<variable name="z" min="-10.12" max="10.05">
</definition>

<definition name="S6" type="asurf">
// http://www.dm.unito.it/modelli/modelli/s6.html
// Conical surface with four double points obtained from the finite real Cayley surface by a homographic transformation or projectivity. 
-x^2 + z x^2 + y^2 z - 2 z^3 + 3 z^2 - z;
<variable name="x" min="-4.14" max="4.03">
<variable name="y" min="-3.13" max="3.04">
<variable name="z" min="-2.62" max="2.85">
</definition>

<definition name="S7" type="asurf">
// http://www.dm.unito.it/modelli/modelli/s7.html
// Three double points
54 x^3 + 18 x^2 z + 108 x^2 - 16 z^3 - 72 z^2 + 792 z  - 162 y^2 x 
+ 18 y^2 z + 108 y^2 - 1512;

<variable name="x" min="-4.01" max="4.03">
<variable name="y" min="-4.13" max="4.14">
<variable name="z" min="-0.85" max="5.86">
</definition>

<definition name="CayleyIV" type="asurf">
//<h3>Class IV: 2 A1 singularity</h3>
//<p>In Cayley's notation: IV=12 - 2C2	
W X Z+Y^2(k Z+l W)+a X^3 +b X^2 Y+c X Y^2 + d Y^3;
X = x + y + z + w;
Y = -x - y + z + w;
Z = x -y -z + w;
W = -x + y - z + w;
w = 1;
<parameter name="a" value="1" />
<parameter name="b" value="0" />
<parameter name="c" value="0" />
<parameter name="d" value="1" />
<parameter name="k" value="1" />
<parameter name="l" value="1" />
<variable name="x" min="-2.14" max="2.03">
<variable name="y" min="-2.13" max="2.04">
<variable name="z" min="-2.12" max="2.05">
</definition>

<definition name="CayleyVIII" type="asurf">
//<p>In Cayley's notation: //VIII=12-3 C2	
Y^3+Y^2(X+Z+W)+4 a X Z W;
X = x + y + z + w;
Y = -x - y + z + w;
Z = x -y -z + w;
W = -x + y - z + w;
w = 1;
<parameter name="a" value="1" />
<variable name="x" min="-2.14" max="2.03">
<variable name="y" min="-2.13" max="2.04">
<variable name="z" min="-2.12" max="2.05">
</definition>

<definition name="CayleyXVI" type="asurf">
//<h3>XVI - 4 A1 singularities</h3>
//<p>In Cayley's notation: //XVI=12-4C2	
W(X Y+X Z+Y Z)+X Y Z;
X = x + y + z + w;
Y = -x - y + z + w;
Z = x -y -z + w;
W = -x + y - z + w;
w = 1;
<variable name="x" min="-2.14" max="2.03">
<variable name="y" min="-2.13" max="2.04">
<variable name="z" min="-2.12" max="2.05">
</definition>

</group>

