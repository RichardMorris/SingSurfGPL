<group name="Simple">
<definition name="Plane" type="asurf">
a x + b y + c z - d;
<parameter name="a" value="1" />
<parameter name="b" value="1" />
<parameter name="c" value="0" />
<parameter name="d" value="0" />

<variable name="x" min="-1.14" max="1.03">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
</definition>

<definition name="Sphere" type="asurf">
(x-x0)^2 + (y-y0)^2 + (z-z0)^2 - r^2;
<variable name="x" min="-1.14" max="1.03">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
<parameter name="r" value="1" />
</definition>

<definition name="Cylinder" type="asurf">
x^2 + y^2 - 1;
<variable name="x" min="-1.14" max="1.03">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
</definition>

<definition name="Ellipsoid" type="asurf">
(x/a)^2+(y/b)^2+(z/c)^2 - 1;
<parameter name="a" value="1" />
<parameter name="b" value="0.5" />
<parameter name="c" value="0.4" />
<variable name="x" min="-1.04" max="1.03">
<variable name="y" min="-1.03" max="1.04">
<variable name="z" min="-1.02" max="1.05">
</definition>

<definition name="Hyperboloid of one sheet" type="asurf">
x^2 - y^2 - z^2 - a;
<parameter name="a" value="-0.1" />
<variable name="x" min="-1.12" max="1.05">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-0.94" max="0.93">
</definition>

<definition name="Hyperboloid of two sheets" type="asurf">
x^2 - y^2 - z^2 - a;
<parameter name="a" value="0.03" />
<variable name="x" min="-1.12" max="1.05">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-0.94" max="0.93">
</definition>

<definition name="hyperbolic paraboloid" type="asurf">
z = x^2/a^2 - y^2/b^2;
<parameter name="a" value="1" />
<parameter name="b" value="1" />
<variable name="x" min="-1.12" max="1.05">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-0.94" max="0.93">
</definition>

<definition name="Monge form surface" type="asurf">
z - (A x^2 + B y^2)/2 - (a x^3 + 3 b x^2 y + 3 c x y^2 + d y^3)/6;
<parameter name="A" value="1">
<parameter name="B" value="-1">
<parameter name="a" value="0.5">
<parameter name="b" value="-0.5">
<parameter name="c" value="0.1">
<parameter name="d" value="3">
<variable name="x" min="-1" max="1">
<variable name="y" min="-1" max="1">
<variable name="z" min="-0.94" max="0.93">
</definition>

</group>

<group name="non-singular">
<definition name="Torus" type="asurf">
// From https://homepage.univie.ac.at/herwig.hauser/gallery.html
(x^2+y^2+z^2+R^2-r^2)^2 - 4 R^2(x^2+y^2);

<parameter name="R" value="0.8" />
<parameter name="r" value="0.2" />
<variable name="x" min="-1.14" max="1.03">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
</definition>

<definition name="Dupin Cyclides" type="asurf">
(r1^2 - dy^2 - (dx + r0)^2) (r1^2 - dy^2 - (dx - r0)^2)*
(x^4+y^4+z^4)+ 2((r1^2 - dy^2 - (dx + r0)^2 )*
(r1^2 - dy^2 - (dx - r0)^2) (x^2 y^2+x^2 z^2+y^2 z^2))+
2 ri^2 ((-dy^2-dx^2+r1^2+r0^2) (2 x dx+2 y dy-ri^2)-4 dy r0^2 y)*
(x^2+y^2+z^2)+ 4 ri^4(dx x+dy y) (-ri^2+dy y+dx x)+
4 ri^4 r0^2 y^2+ri^8-0;
<variable name="x" min="-2.01" max="2.22">
<variable name="y" min="-2.03" max="2.04">
<variable name="z" min="-2.05" max="2.16">
<parameter name="r0" value="2.1" />
<parameter name="r1" value="2" />
<parameter name="dx" value="2" />
<parameter name="dy" value="0" />
<parameter name="ri" value="2" />
</definition>

<definition name="Squared off Sphere" type="asurf">
x^4 + y^4 + z^4 - 1;
<variable name="x" min="-1.14" max="1.13">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
</definition>

<definition name="Bumpy sphere" type="asurf">
x^2 + y^2 + z^2 - 1 
+ 1/3 cube (x y z)
+ 1/3 orange (x^3 - 3 x y^2)
+ 1/3 tennis (x y^2)
+ 1/3 revolve (x^3);
<variable name="x" min="-1.14" max="1.13">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
<parameter name="cube" value="0.1" />
<parameter name="orange" value="0" />
<parameter name="tennis" value="0" />
<parameter name="revolve" value="0" />
</definition>

<definition name="Bumpy elipsoid" type="asurf">
1/a x^2 + 1/b y^2 + 1/c z^2 - 1 
+ 1/3 lambda (x y z);
<variable name="x" min="-1.14" max="1.13">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
<parameter name="a" value="1" />
<parameter name="b" value="3" />
<parameter name="c" value="5" />
<parameter name="lambda" value="0.1" />
</definition>

</group>

