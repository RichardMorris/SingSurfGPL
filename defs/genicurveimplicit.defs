<definition name="Implicit" type="genICurve"  opType="asurf">
[Sxx,Sxy,Sxz,Syy,Syz,Szz, Sx,Sy,Sz];
Sxx = diff(Sx,x);
Sxy = diff(Sx,y);
Sxz = diff(Sx,z);
Syy = diff(Sy,y);
Syz = diff(Sy,z);
Szz = diff(Sz,z);
Sx = diff(S,x);
Sy = diff(S,y);
Sz = diff(S,z);

<variable name="S" type="asurf">
<variable name="x">
<variable name="y">
<variable name="z">
<option name="colour" value="Red">
<option name="orientation" value="Implicit">
</definition>
