<variable name="z">
<variable name="w">
</definition>
    
<definition name="A1 A3" type="p3">
//In Cayley's notation: x=12-B4-C2	
w x z+(x+z)(y^2-x^2);
<variable name="x">
<variable name="y">
<variable name="z">
<variable name="w">
</definition>

<definition name="A5" type="p3">
//In Cayley's notation: xI=12-B6		
