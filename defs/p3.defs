S6 = x^6+y^6+z^6+w^6 + 15( x^2 y^2 z^2 + x^2 y^2 w^2 + x^2 z^2 w^2 + y^2 z^2 w^2);
<variable name="x">
<variable name="y">
<variable name="z">
<variable name="w">
</definition>
     
<definition name="Barth Sextic"type="p3">
4 (phi^2 x^2-y^2) (phi^2 y^2 - z^2)(phi^2 z^2-x^2) - (1+2 phi)(x^2+y^2+z^2-w^2)^2 w^2;
phi = (1+sqrt(5))/2;
<variable name="x">
<variable name="y">
<variable name="z">
<variable name="w">
</definition>
	
<definition name="Sarti's Octic 72" type="p3">
S8 -3/4 Q8;
Q8 = (x^2+y^2+z^2+w^2)^4;
S8 = x^8+y^8+z^8+w^8
	+7 z^4*x^4                    //  x      z
	+7 x^4*w^4                    //  x          w
	+7 z^4*y^4                    //      y  z
	+7 y^4*w^4                    //      y      w
	+14 z^4*w^4                   //2        z   w
	+14 x^4*y^4                   //2 x   y
	+42 z^2*x^4*w^2
	+42 z^2*y^4*w^2
	+42 z^4*x^2*y^2
	+42 x^2*y^2*w^4
	-84 z^2*x^2*y^2*w^2;
<variable name="x">
<variable name="y">
<variable name="z">
<variable name="w">
</definition>
	
<definition name="Sarti's Octic 144" type="p3">
S8 -9/16 Q8;
Q8 = (x^2+y^2+z^2+w^2)^4;
S8 = x^8+y^8+z^8+w^8
	+7 z^4*x^4                    //  x      z
	+7 x^4*w^4                    //  x          w
	+7 z^4*y^4                    //      y  z
	+7 y^4*w^4                    //      y      w
	+14 z^4*w^4                   //2        z   w
	+14 x^4*y^4                   //2 x   y
	+42 z^2*x^4*w^2
	+42 z^2*y^4*w^2
	+42 z^4*x^2*y^2
	+42 x^2*y^2*w^4
	-84 z^2*x^2*y^2*w^2;
<variable name="x">
<variable name="y">
<variable name="z">
<variable name="w">
</definition>
	
<definition name="EndraÃŸ's Octic" type="p3">
//# Degree 8, 168 nodes
64(x^2-w^2)(y^2-w^2)((x+y)^2-2 w^2)((x-y)^2-2 w^2)
	-( a (x^2+y^2)^2 +(b z^2+c w^2)(x^2+y^2)
    -16 z^4  +d z^2 w^2 +f w^4 )^2 - 0;
a = -4(1+sqrt(2));
b =  8(2+sqrt(2));
c =  2(2+7sqrt(2));
d =  8(1-2sqrt(2));
f =  -(1+12sqrt(2));
<variable name="x">
<variable name="y">
<variable name="z">
<variable name="w">
</definition>		
	
<definition name="Barth's Dectic" type="p3">
//# Degree 10, 345 nodes
8 (x^2-t^4 y^2)(y^2 -t^4 z^2)(z^2-t^4 x^2)
	* (x^4 + y^4 + z^4 - 2 x^2 y^2 - 2 x^2 z^2 - 2 y^2 z^2)
+ (3+5 t) (x^2+y^2+z^2-w^2)^2 (x^2 + y^2 + z^2 - (2-t) w^2)^2 w^2;
t = (1+sqrt(5))/2;
<variable name="x">
<variable name="y">
<variable name="z">
<variable name="w">
</definition>
	
<definition name="Sarti's Dodectic" type="p3">
243 S12 - 22 Q12;
Q12 = (x^2+y^2+z^2+w^2)^6;
S12 = 33 sqrt(5) ( s23m + s34m + s42m)
    + 19(s23p + s34p + s42p)
    + 10 s234 - 14 s10 + 2 s11 - 6 s12
    - 352 s51 + 336 l5^2 l1 + 48 l2 l3 l4;
s10 = l1 (l2 l3 + l2 l4 + l3 l4);
s11 = l1^2 ( l2 + l3 + l4);
s12 = l1 (l2^2 + l3^2 +l4^2);
s51 = l5^2 ( l2 + l3 + l4);
s234 = l2^3 + l3^3 + l4^3;
s23p = l2^2 l3 + l2 l3^2;
s23m = l2^2 l3 - l2 l3^2;
s34p = l3^2 l4 + l3 l4^2;
s34m = l3^2 l4 - l3 l4^2;
s42p = l4^2 l2 + l4 l2^2;
s42m = l4^2 l2 - l4 l2^2;
l1 = x^4 + y^4 + z^4 + w^4;
l2 = x^2 y^2 + z^2 w^2;
l3 = x^2 z^2 + y^2 w^2;
l4 = x^2 w^2 + y^2 z^2;
l5 = x y z w;
<variable name="x">
<variable name="y">
<variable name="z">
<variable name="w">
</definition>
	
<definition name="Sarti's Dodectic 360" type="p3">
25 S12 - 2 Q12;
Q12 = (x^2+y^2+z^2+w^2)^6;
S12 = 33 sqrt(5) ( s23m + s34m + s42m)
    + 19(s23p + s34p + s42p)
